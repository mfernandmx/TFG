doctype html
html
    head
        title #{rTitle}
        // - link(rel='stylesheet', href='../stylesheets/style.css')
    body
        h1 #{rTitle}

        a(href = projectHomePage) #{projectName}

        h2 #{rUri}

        if rTypes.length > 0
            h2 Tipos
            ul
                each type in rTypes
                    li
                        a(href=type.url) #{type.prefix}:#{type.value}

        block content

        script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js")
        script(type="text/javascript", src="../client/client.js")

        script(type="text/javascript").
            point = "#{geoPoint}";
            figure = "#{geoFigure}";
            initializePoint(point);
            initializeFigure(figure);


        script(async defer, src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBt5CcFk1btCb9QpnwAmEtPNI7k4U3loEU&callback=initMap")